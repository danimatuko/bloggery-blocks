{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "bloggery/fancy-header",
    "title": "Bloggery Header",
    "category": "text",
    "icon": "heading",
    "description": "Add a header with a hover effect",
    "keywords": [
        "title",
        "heading"
    ],
    "version": "1",
    "textdomain": "bloggery-blocks",
    "editorScript": "file:./index.js",
    "attributes": {
        "content": {
            "type": "string",
            "source": "html",
            "selector": "h2"
        },
        "underline_color": {
            "type": "string",
            "default": "#f87171"
        }
    },
    "style": "file:./index.css"
}